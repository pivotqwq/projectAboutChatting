{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8083"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Mongo": "mongodb://YOUR_MONGO_CONNECTION_STRING",
    "Redis": "YOUR_REDIS_CONNECTION_STRING"
  },
  "Mongo": {
    "Database": "YOUR_DATABASE_NAME"
  },
  "Jwt": {
    "SecretKey": "YOUR_JWT_SECRET_KEY_HERE",
    "Issuer": "UserManagerAPI",
    "Audience": "UserManagerClient",
    "ExpirationInMinutes": 60
  }
}
